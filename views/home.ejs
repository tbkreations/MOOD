<div class="homeMain">
  <div class="sideNav">
    <h3 id="sideNavTitle">MOOD</h3>
    <h6 id="sideNavSubTitle">A Social Music Experience</h6>
    <ul class="navList">
      <li class="navListItem navItemSelected"><a href="/home">Home</a></li>
      <li class="navListItem"><a href="/mood">My MOOD</a></li>
      <li class="navListItem"><a href="/artists">My Artists</a></li>
      <li class="navListItem"><a href="/songs">My Songs</a></li>
    </ul>
  </div>
  <div class="centerContainer">
    <div class="profileContainer">
      <h3 id="profileName">hello, <%= name %></h3>
    </div>
    <h2 class="containerHeading">More from your favorites</h2>
    <div class="topArtistContainer">
      <% topArtists.forEach((artist) => { %>
      <div data-id="<%=artist.id%>" class="albumContainer">
        <img
          class="artistPlaylistCover"
          src="<%=artist.images[0].url%>"
          alt="<%=artist.name%> image"
        />
        <div class="albumName"><%=artist.name%></div>
      </div>
      <% }) %>
    </div>
  </div>
</div>
